#include <stdio.h>
#include <stdlib.h>


int main()
{
    int firstInteger, secondInteger, check;
    printf("Input any 2 integer to add, multiple, divide and subtract: \n");
    check = scanf("%d %d", &firstInteger, &secondInteger);


if(!check)
    {
    return printf("Invalid input");
    }

    system("cls");
    printf("Input any 2 integers:%d,%d\n\n", firstInteger, secondInteger);
    printf("ADD = %d\n", firstInteger + secondInteger);
    printf("MUL = %d\n", firstInteger * secondInteger);
    printf("DIV = %d\n", firstInteger / secondInteger);
    printf("SUB = %d\n", firstInteger - secondInteger);

return 0;
}
