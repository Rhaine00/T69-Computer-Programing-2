#include <stdio.h>
#include <stdlib.h>

int main()
{
    int age, check;
    char person[99];
    printf("Name: ");
    scanf("%s", &person);
    printf("Age: ");
    check = scanf("%d", &age);

if(!check)
    {
    printf("Invalid Age, Please Try Again.\n\n");
    return 0;
    }
    if( age < 18 )
    {
    printf("You are not eligible for voting.\n\n");
    }
    else if(age >= 18)
    {
    printf("You are eligible for voting.\n\n");
    }

return 0;

}
